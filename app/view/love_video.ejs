<section class="view view-2 view_slide">
    <div class="view_cont">
        <input type="text" id="search">
        <div class="view_cont-wrap">
        <% videos.map((video, i) => {%>
        <input type="checkbox" value="" class="view_itm-in" id="video_item_<%- i %>">
        <label for="video_item_<%- i %>">
                <articl class="view_itm">
                    <div class="view_itm-img">
                        <a class="view_itm-link">
                            <img src="<%- video.image.small%>"
                                 alt="" class="view_itm-img-cont">
                        </a>
                    </div>
                    <p class="view_itm-title">
                        <%= video.title %>
                    </p>
                    <p class="view_itm-info"> <%= video.year %>,  <%= video.genres[0] %></p>
                </articl>
        </label>
        <% }) %>
        </div>
    </div>
</section>